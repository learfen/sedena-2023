<div id="alertBox" style="display: none;">
    <span class="alertBox-close"> x </span>
    <span id="alertBoxText"></span>
</div>

<style>
	#alertBox{
		position: fixed;
		top: 0;
		z-index: 10;
		width: 100%;
	}
	.alertBox-close{
        display: inline-block;
        width: 1.5rem;
        height: 1.5rem;
        color: brown;
        margin: 0px 2rem;
        font-weight: bold;
		cursor: pointer;
		font-size: 1.25rem;
		line-height: 1.25rem;
		text-transform: uppercase;
    }
	.alertBox-close:hover{
		color:#fff
	}
	.delete{
		position:relative;
		float: left;
		margin-top:.5rem;
		margin-right: -4.5rem;
		border-radius:50%;
		background:red;
		border:1px red solid;
		color:yellow;
		width:2rem;
		height:2rem;
		font-family:arial;
		display:flex;
		align-items:center;
		justify-content:center;
		cursor: pointer;
		font-size: 1.25rem;
		text-transform: uppercase;
		z-index: 1;
	}
	.delete:hover{
		color:#fff;
		border:1px #fff solid
	}
	.table-video td.text{
		padding: 1rem;
	    word-break: break-all;
	}
</style>


<script>
	const alertBox = document.querySelector("#alertBox")
	const alertBoxText = document.querySelector("#alertBoxText")

	alertBox.children[0].onclick = ( e ) =>  { e.target.parentNode.style.display = "none" }

	function alertSuccess(txt){
		alertBox.style.display = "flex"
		alertBox.style.background="green"
		alertBoxText.innerHTML = txt
	}

	function alertWarning(txt){
		alertBox.style.display = "flex"
		alertBox.style.background="orange"
		alertBoxText.innerHTML = txt
	}
	const expresionRegularFilesName = /^[0-9a-zA-Z\.\-\_]+$/
	let inputsFilesForValidation = document.querySelectorAll('input[file]')
	for(let input of inputsFilesForValidation){
		input.addEventListener('change', e => {
			for(file of e.target.files){
				if(!expresionRegularFilesName.test(file.name)){
					inputsFilesForValidation = []
					e.preventDefault()
					alertWarning("Nombre de archivo no valido solo a-z A-Z 0-9 - _ ("+file.name+")")
				}
			}
		})
	}
</script>