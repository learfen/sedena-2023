<div class="row justify-content-center heading-page bg-gray text-center pt-4 mt-4">
	<div class="col-5">
		<span class="total-elements"><%= size %> elementos</span>
		<nav class="d-md-inline-flex" aria-label="Page navigation example">
			<ul class="pagination justify-content-center">
			  <li class="page-item">
				<a class="page-link" href="?page=1" aria-label="Previous">
				  <span aria-hidden="true">&laquo;</span>
				</a>
			  </li>
			  <li class="page-item">
				<a class="page-link" href="?page=<%= prev %>" aria-label="Previous">
				  <span aria-hidden="true">&lt;</span>
				</a>
			  </li>
			  <span class="page-count"> <%= paginationText %></span>
			  <li class="page-item">
				<a class="page-link" href="?page=<%= next %>" aria-label="Next">
				  <span aria-hidden="true">&gt;</span>
				</a>
			  </li>
			  <li class="page-item">
				<a class="page-link" href="?page=<%= last %>" aria-label="Next">
				  <span aria-hidden="true">&raquo;</span>
				</a>
			  </li>
			</ul>
		  </nav>
	</div>
</div>